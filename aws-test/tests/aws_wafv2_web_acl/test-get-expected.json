[
  {
    "akas": ["{{ output.resource_aka.value }}"],
    "default_action": {
      "Allow": {
        "CustomRequestHandling": null
      },
      "Block": null
    },
    "description": "Example of a managed rule.",
    "name": "{{ resourceName }}",
    "scope": "REGIONAL",
    "tags_src": [
      {
        "Key": "name",
        "Value": "{{ resourceName }}"
      }
    ],
    "title": "{{ resourceName }}",
    "visibility_config": {
      "CloudWatchMetricsEnabled": false,
      "MetricName": "friendly-metric-{{ resourceName }}",
      "SampledRequestsEnabled": false
    }
  }
]
